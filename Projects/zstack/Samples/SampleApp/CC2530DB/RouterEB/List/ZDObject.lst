    200          /*********************************************************************
    201           * @fn      ZDOManagedScan_Next()
    202           *
    203           * @brief   Setup a managed scan.
    204           *
    205           * @param   none
    206           *
    207           * @return  none
    208           */
    209          static void ZDOManagedScan_Next( void )
    210          {
    211            // Is it the first time
    212            if ( managedScanNextChannel == 0 && managedScanTimesPerChannel == 0 )
    213            {
    214              // Setup the defaults
    215              managedScanNextChannel  = 1;
    216          
    217              while( managedScanNextChannel && (zgDefaultChannelList & managedScanNextChannel) == 0 )
    218                managedScanNextChannel <<= 1;
    219          
    220              managedScanChannelMask = managedScanNextChannel;
    221              managedScanTimesPerChannel = MANAGEDSCAN_TIMES_PRE_CHANNEL;
    222            }
    223            else
    224            {
    225              // Do we need to go to the next channel
    226              if ( managedScanTimesPerChannel == 0 )
    227              {
    228                // Find next active channel
    229                managedScanChannelMask  = managedScanNextChannel;
    230                managedScanTimesPerChannel = MANAGEDSCAN_TIMES_PRE_CHANNEL;
    231              }
    232              else
    233              {
    234                managedScanTimesPerChannel--;
    235          
    236                if ( managedScanTimesPerChannel == 0 )
    237                {
    238                  managedScanNextChannel  <<= 1;
    239                  while( managedScanNextChannel && (zgDefaultChannelList & managedScanNextChannel) == 0 )
    240                    managedScanNextChannel <<= 1;
    241          
    242                  if ( managedScanNextChannel == 0 )
    243                    zdoDiscCounter  = NUM_DISC_ATTEMPTS + 1; // Stop
    244                }
    245              }
    246            }
    247          }
    248          #endif // MANAGED_SCAN
